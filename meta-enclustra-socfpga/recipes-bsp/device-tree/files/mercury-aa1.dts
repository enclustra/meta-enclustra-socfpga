/dts-v1/;

#include "socfpga_arria10.dtsi"

/ {
	model = "Mercury+ AA1";
	compatible = "altr,socfpga-arria10", "altr,socfpga";

	aliases {
		serial0 = &uart1;
	};

	chosen {
		bootargs = "earlyprintk";
		stdout-path = "serial0:115200n8";
	};

	memory@0 {
		name = "memory";
		device_type = "memory";
		reg = <0x0 0x80000000>; /* 2GB */
	};

	soc {
		clkmgr@ffd04000 {
			clocks {
				osc1 {
					clock-frequency = <33000000>;
				};
			};
		};
	};
};

&i2c1 {
	status = "okay";
};

&qspi {
	status = "okay";
};

&uart1 {
	status = "okay";
};

&watchdog1 {
	status = "okay";
};
